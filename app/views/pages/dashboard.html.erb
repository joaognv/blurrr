<div class="profile-show">
  <div class="profile-header">
    <div class="avatar-medium avatar-drop-on-line" style="background-image: url(<%= @current_user.profile.photo.url ? current_user.profile.photo.url : 'http://dl1.cbsistatic.com/i/2017/10/19/925f2651-018c-4d76-baf2-3ce4de2ecdb4/a9d0a0ebc85b66e01318f74b0be11da8/imgingest-7427792717055670780.png';  %> );" >
    </div>
  </div>

<section id="tabs">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">

        <ul class="nav nav-tabs nav-fill profile-nav" id="nav-tab" role="tablist">
          <li class="active"><a class="nav-item nav-link" id="nav-profile-tab" data-toggle="tab" href="#nav-profile" role="tab" aria-controls="nav-profile" aria-selected="false">Profile</a></li>
          <li><a class="nav-item nav-link" id="nav-contact-tab" data-toggle="tab" href="#nav-contact" role="tab" aria-controls="nav-contact" aria-selected="false">Matches</a></li>
          <li><a class="nav-item nav-link" id="nav-about-tab" data-toggle="tab" href="#nav-about" role="tab" aria-controls="nav-about" aria-selected="false">Questions</a></li>
        </ul>

        <div class="tab-content py-3 px-3 px-sm-0" id="nav-tabContent">

          <div class="tab-pane active" id="nav-profile" role="tabpanel" aria-labelledby="nav-profile-tab">
            <%= render "pages/dashboard/profile_tab_pane" %>
          </div>

          <div class="tab-pane fade" id="nav-contact" role="tabpanel" aria-labelledby="nav-contact-tab">

            <% @matches.each do |m|  %>
              <div class="match-box">
                <div class="profile-roww">
                  <%= link_to profile_path(m.users.where.not(id: current_user.id).first.profile) do %>
                    <div class="avatar-extra-small-matches" style="background-image: url(<%= m.users.where.not(id: current_user.id).first.profile.photo.url  %> );" >
                    </div>
                  <% end %>
                </div>

                <div class="bubbble-right chat-bubble-match">
                  <%= link_to match_path(m) do %>
                    <div class="chat-bubble-match">
                      <%= m.last_message %>
                    </div>
                  <% end %>
                </div>
              </div>
            <% end %>
          </div>

          <div class="tab-pane fade" id="nav-about" role="tabpanel" aria-labelledby="nav-about-tab"> There are currently no questions to view. </div>

        </div>

      </div>
    </div>
  </div>
</section>


</div>

